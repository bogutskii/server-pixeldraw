{
  "name": "server-express",
  "version": "1.0.0",
  "description": "server for pixel draw",
  "main": "src/index.js",
  "scripts": {
    "devstart": "nodemon --inspect --exec babel-node src/index.js",
    "devbuild": "babel src/index.js -d  build",
    "babel-node": "babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'",
    "test": "echo \"Error: no test specified\" && exit 1",

    "start": "npm run build && node ./build/index",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src",
    "clean": "rm -rf ./build && mkdir build",
    "heroku-postbuild": "npm install && npm run start"
  },
  "engines": {
    "node": "12.x"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  }
}
